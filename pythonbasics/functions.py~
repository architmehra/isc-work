def double_it(number): 
    return number*2

print double_it(3)
print double_it(3.4)
print double_it('3.4')

def calc_hypo(a,b):
    if type(a) not in (int,float) or type(b) not in (int,float):
        print "Bad argument" 
        return False 
    if a <=0 or b <=0:
        print "Bad argument" 
        return False 
    hypo = (a**2 + b**2)**0.5
    return hypo 

print calc_hypo(3,4)
calc_hypo("hi","bye")

 
